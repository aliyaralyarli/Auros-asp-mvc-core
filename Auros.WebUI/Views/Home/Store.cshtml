
@{
    ViewData["Title"] = "Store";
    Layout = "_StoreLayout";
}
<section class="coming-soon">
        <div>
            <h2>We are opening up soon!</h2>
            <div class="countdown">
                <div class="container-day">
                    <h3 class="day">01</h3>
                    <h3>Day</h3>
                </div>
                <div class="container-hour">
                    <h3 class="hour">01</h3>
                    <h3>Hour</h3>
                </div>
                <div class="container-minute">
                    <h3 class="minute">01</h3>
                    <h3>Minute</h3>
                </div>
                <div class="container-second">
                    <h3 class="second">01</h3>
                    <h3>Second</h3>
                </div>
            </div>
        </div>
        <img class="waiting" src="~/Assets/img/website-maintenance.svg" alt="Image">
    </section>
    <script src="~/Assets/JS/app.js"></script>